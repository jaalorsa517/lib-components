{
  "name": "@jaalorsa/j5-components",
  "version": "2.1.0",
  "private": false,
  "description": "Libreria de componentes web para ayudar al desarrollador en su día a día. Como la librería está hecha en vanilla, se puede utilizar sin importar el framework de javascript.",
  "files": [
    "components"
  ],
  "type": "module",
  "types": "./components/main.d.ts",
  "main": "./components/j5-components.umd.cjs",
  "module": "./components/j5-components.js",
  "exports": {
    ".": {
      "types": "./components/main.d.ts",
      "import": "./components/j5-components.js",
      "require": "./components/j5-components.umd.cjs"
    }
  },
  "author": "jaalorsa517",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaalorsa517/j5-components/issues"
  },
  "homepage": "https://jaalorsa.com/lib-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaalorsa517/j5-components.git"
  },
  "keywords": [
    "webcomponents",
    "web",
    "component",
    "typescript",
    "javascript"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.2",
    "@vitest/coverage-v8": "^3.0.5",
    "jsdom": "^26.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  }
}
